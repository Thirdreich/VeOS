{
    "explorer": {
        "theme": {
            "default": "webdows",
            "extraCSS": ""
        },
		"startup": [
			"webdows/belowaverage/explorer/piwik.js",
			"webdows/config/explorer.startup.js",
			"webdows/welcome.js",
			"webdows/resources/explorer/animation.js"
		],
		"startmenu": [
			{
				"title": "About Webdows",
				"icon": "webdows/resources/icons/info.ico",
				"file": "webdows/webver.js"
			}, {
				"title": "Personalize",
				"icon": "webdows/resources/icons/pers.ico",
				"file": "webdows/personalize.js"
			}, {
				"title": "Command Prompt",
				"icon": "webdows/resources/icons/scre.ico",
				"file": "webdows/cmd.js"
			}, {
				"title": "Registry Editor",
				"icon": "webdows/resources/icons/rege.ico",
				"file": "webdows/regedit.js"
			}, {
				"title": "Calculator",
				"icon": "webdows/resources/icons/calc.ico",
				"file": "webdows/calc.js"
			}, {
				"title": "Notepad",
				"icon": "webdows/resources/icons/note.ico",
				"file": "webdows/notepad.js"
			}, {
				"title": "Run",
				"icon": "webdows/resources/icons/runi.ico",
				"file": "webdows/run.js"
			}, {
				"title": "Task Manager",
				"icon": "webdows/resources/icons/task.ico",
				"file": "webdows/taskmgr.js"
			}, {
				"title": "Youtube Extractor",
				"icon": "programs/Youtube Extractor/logo.png",
				"file": "programs/Youtube Extractor/index.js"
			}, {
				"title": "Web Explorer",
				"icon": "programs/Web Explorer/1.png",
				"file": "programs/Web Explorer/we.js"
			}, {
				"title": "YouTube Sync",
				"icon": "programs/youtube-sync/img/icon.png",
				"file": "programs/youtube-sync/index.js"
			}
		]
    },
    "system": {
		"webldr": {
			"exclude": [
				"index.html",
				"Public/",
				"programs/",
				"webdows/resources/icons/",
				"webdows/config/",
				"webdows/system32/"
			]
		},
		"startup": [
			"webdows/belowaverage/system/cordova_check.js",
			"webdows/belowaverage/system/piwik.js",
			"webdows/explorer.js"
		],
		"services": {
			"explorer_time": {
				"autoStart": "true",
				"interval": "1000",
				"path": "webdows/services/explorer/time.js"
			}
		},
        "files": {
            "ext": {
                "txt": {
                    "icon": "webdows/resources/icons/note.ico",
                    "defaultProgram": "webdows/notepad.js"
                },
                "js": {
                    "icon": "webdows/resources/icons/exei.ico"
                },
                "png": {
                    "icon": "webdows/resources/icons/pngi.ico",
                    "defaultProgram": ""
                },
                "gif": {
                    "icon": "webdows/resources/icons/gifi.ico",
                    "defaultProgram": ""
                },
                "jpg": {
                    "icon": "webdows/resources/icons/jpgi.ico",
                    "defaultProgram": ""
                },
                "ico": {
                    "icon": "webdows/resources/icons/bmpi.ico",
                    "defaultProgram": ""
                }
            }
        }
    },
	"software": {
		"belowaverage-org": {
			"weblogon": {
				"domain": "belowaverage.org",
				"registry": "https://api.belowaverage.org/v1/webdows/registry/"
			}
		}
	}
}